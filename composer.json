{
    "name": "holokron/json-patch",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "description": "Library to handle JSON patch requests compatibile with RFC 6902",
    "keywords": [
        "json",
        "json-patch",
        "rfc6902"
    ],
    "authors": [
        {
            "name": "Michał Tęczyński",
            "email": "michal.teczynski@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {        
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "~2.3",
        "phpstan/phpstan": "^0.6.4",
        "phpmetrics/phpmetrics": "^2.2.0",
        "michalv8/xpmock": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Holokron\\JsonPatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Holokron\\JsonPatch\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
